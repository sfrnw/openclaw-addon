{
  "name": "OpenClaw AI Assistant",
  "version": "1.0.3",
  "slug": "openclaw",
  "description": "AI personal assistant with Telegram, Email, and Home Assistant integration",
  "arch": ["aarch64", "armv7"],
  "url": "https://github.com/openclaw/openclaw",
  "startup": "application",
  "boot": "auto",
  "ports": {
    "18789/tcp": 18789
  },
  "ports_description": {
    "18789/tcp": "OpenClaw Gateway UI"
  },
  "homeassistant": "2023.1.0",
  "host_network": false,
  "privileged": [],
  "devices": [],
  "options": {
    "telegram_token": "",
    "telegram_allowed_users": "",
    "gateway_token": "",
    "timezone": "Europe/Lisbon"
  },
  "schema": {
    "telegram_token": "str",
    "telegram_allowed_users": "str",
    "gateway_token": "str",
    "timezone": "str?",
    "gmail_email": "str?",
    "gmail_app_password": "str?",
    "notion_api_key": "str?",
    "homeassistant_url": "str?",
    "homeassistant_token": "str?"
  },
  "init": false,
  "map": ["ssl:ro"],
  "build_from": {
    "aarch64": "node:20-alpine",
    "armv7": "node:20-alpine"
  }
}
