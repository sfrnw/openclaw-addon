{
  "name": "OpenClaw AI Assistant",
  "version": "3.0.2",
  "slug": "openclaw",
  "description": "Official OpenClaw AI assistant based on docker-setup.sh",
  "arch": ["aarch64", "armv7"],
  "url": "https://github.com/sfrnw/openclaw-addon",
  "startup": "application",
  "boot": "auto",
  "ports": {
    "18789/tcp": 18789
  },
  "homeassistant": "2023.1.0",
  "options": {
    "telegram_token": ""
  },
  "schema": {
    "telegram_token": "str?"
  },
  "init": false,
  "map": ["config:ro"],
  "homeassistant_api": true,
  "path": {
    "/home/node/.openclaw": "/data"
  }
}
