{
  "name": "OpenClaw AI Assistant",
  "version": "3.1.1",
  "slug": "openclaw",
  "description": "OpenClaw AI with local Ollama (phi3:mini) - no API keys needed",
  "arch": ["aarch64"],
  "url": "https://github.com/sfrnw/openclaw-addon",
  "startup": "services",
  "boot": "auto",
  "ports": {
    "18789/tcp": 18789,
    "11434/tcp": 11434
  },
  "homeassistant": "2023.1.0",
  "options": {
    "telegram_token": "",
    "model": "phi3:mini"
  },
  "schema": {
    "telegram_token": "str?",
    "model": "str?"
  },
  "init": false,
  "map": ["data"],
  "homeassistant_api": true,
  "hassio_api": true,
  "stage": "experimental"
}
