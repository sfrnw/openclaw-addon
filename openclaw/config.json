{
  "name": "OpenClaw AI Assistant",
  "version": "3.0.7",
  "slug": "openclaw",
  "description": "OpenClaw AI with Ollama (local) or Qwen (free cloud)",
  "arch": ["aarch64", "armv7"],
  "url": "https://github.com/sfrnw/openclaw-addon",
  "startup": "application",
  "boot": "auto",
  "ports": {
    "18789/tcp": 18789
  },
  "homeassistant": "2023.1.0",
  "options": {
    "telegram_token": "",
    "ollama_url": ""
  },
  "schema": {
    "telegram_token": "str?",
    "ollama_url": "str?"
  },
  "init": false,
  "map": ["data"],
  "homeassistant_api": true,
  "hassio_api": true,
  "stage": "stable"
}
